{
	"name": "sveltekit-starter",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "cross-env vite dev",
		"build": "cross-env vite build",
		"preview": "cross-env vite preview",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:e2e": "playwright test",
		"test:unit": "vitest run \"src/lib/**/tests/unit/**/*.test.ts\"",
		"test:integration": "vitest run \"src/lib/**/tests/integration/**/*.integration.test.ts\"",
		"test": "vitest run"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@playwright/test": "^1.49.1",
		"@skeletonlabs/skeleton": "^2.10.3",
		"@skeletonlabs/tw-plugin": "^0.4.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^5.5.2",
		"@sveltejs/kit": "^2.9.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.6",
		"@tsconfig/svelte": "^5.0.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/node": "^22.10.5",
		"@vitest/ui": "^2.1.8",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"esbuild": "^0.24.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.45.1",
		"globals": "^15.0.0",
		"jsdom": "^26.0.0",
		"postcss": "^8.4.49",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.9.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.16",
		"tsx": "^4.19.2",
		"typescript": "^5.7.2",
		"vite": "^6.0.0",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"lucide-svelte": "^0.469.0",
		"postgres": "^3.4.5",
		"zod": "^3.23.8"
	},
	"overrides": {
		"vite": {
			"rollup": "npm:@rollup/wasm-node"
		}
	},
	"optionalDependencies": {
		"esbuild": "npm:esbuild-linux-arm64@latest",
		"esbuild-darwin-arm64": "npm:esbuild-darwin-arm64@latest",
		"esbuild-darwin-x64": "npm:esbuild-darwin-x64@latest",
		"esbuild-linux-x64": "npm:esbuild-linux-x64@latest",
		"esbuild-wasm": "^0.15.0",
		"esbuild-win32-x64": "npm:esbuild-win32-x64@latest"
	}
}
